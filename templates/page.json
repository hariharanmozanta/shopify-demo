        "custom_liquid": "<div class=\"container\"><div class=\"row\"><div class=\"mx-auto col-md-6 col-lg-4\"><h1 id=\"recover\" tabindex=\"-1\">\n                {% comment %} {{ 'customer.recover_password.title' | t }} {% endcomment %}\n              <\/h1>\n              <div>\n                {% comment %} <p>\n                  {{ 'customer.recover_password.subtext' | t }}\n                <\/p> {% endcomment %}\n                <div class=\"text-center\"><img alt=\"\" loading=\"lazy\" width=\"300\" height=\"238\" decoding=\"async\" data-nimg=\"1\" class=\"reset-image\" src=\"https:\/\/cdn.shopify.com\/s\/files\/1\/0667\/1299\/4033\/files\/reset-password.svg?v=1696493826\" style=\"color: transparent;\"><h5 class=\"fw-semibold mt-4 mb-2\" a-id=\"fp-s1fop-t\"> Reset Your Password<\/h5><p class=\"mt-1 mb-3\">Kindly provide the Email address to be used for resetting your password.<\/p><\/div>\n                {%- form 'recover_customer_password' -%}\n                {% assign recover_success = form.posted_successfully? %}\n                {% comment %} <div class=\"field\"> {% endcomment %}\n                <div class=\"mb-3\">\n                  <label class=\"form-label\">Email address <span class=\"text-danger\">*<\/span><\/label>\n                  <input type=\"email\" class=\"form-control\" value=\"\" name=\"email\" id=\"RecoverEmail\" autocorrect=\"off\" autocapitalize=\"off\"\n                    autocomplete=\"email\" {% if form.errors %} aria-invalid=\"true\"\n                    aria-describedby=\"RecoverEmail-email-error\" autofocus {% endif %}\n                    placeholder=\"*******38@gmail.com\" required>\n                  {% comment %} <label for=\"RecoverEmail\">\n                    {{ 'customer.login_page.email' | t }}\n                  <\/label> {% endcomment %}\n                <\/div>\n                {%- if form.errors -%}\n                <small id=\"RecoverEmail-email-error\" class=\"form__message\">\n                  <svg aria-hidden